<keymap version="1" name="Shane's Custom Keymap" parent="Default for XWin">
  <action id="Back">
    <mouse-shortcut keystroke="button4" />
    <keyboard-shortcut first-keystroke="alt open_bracket" />
  </action>
  <action id="CloseContent">
    <keyboard-shortcut first-keystroke="ctrl f4" />
    <keyboard-shortcut first-keystroke="ctrl w" />
  </action>
  <action id="EditorSelectWord">
    <keyboard-shortcut first-keystroke="alt w" />
  </action>
  <action id="Forward">
    <mouse-shortcut keystroke="button5" />
    <keyboard-shortcut first-keystroke="alt close_bracket" />
  </action>
  <action id="PrettierImportCodeStyleAction">
    <keyboard-shortcut first-keystroke="shift ctrl alt p" />
  </action>
  <action id="ReformatCode" />
  <action id="ReformatWithPrettierAction">
    <keyboard-shortcut first-keystroke="ctrl alt l" />
  </action>
  <action id="Vcs.ReformatCommitMessage" />
</keymap>